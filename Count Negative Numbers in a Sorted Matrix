class Solution {
public:
    int countNegatives(vector<vector<int>>& grid) {
        int count=0;
        int size1=grid.size();
        for(int i=0;i<size1;i++){
            int size2=grid[i].size();
            for(int j=0;j<size2;j++){
              if(grid[i][j]<0)
              count++;
            }
        }
        return count;
    }
};
